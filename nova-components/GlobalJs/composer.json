{
    "name": "rurbani/global-js",
    "description": "A Laravel Nova asset.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rurbani\\GlobalJs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rurbani\\GlobalJs\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
